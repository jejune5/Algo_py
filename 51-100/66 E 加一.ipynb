   "source": [
    "def plusOne(digits):\n",
    "    orig = 0\n",
    "    for i in range(0,len(digits)):\n",
    "        orig += digits[i]*(10**(len(digits) - i - 1))\n",
    "        \n",
    "    res = list(map(int, str(orig + 1)))\n",
    "    \n",
    "    return res\n",
    "\n",
    "\n",
    "\n",
    "digits = [1,2,3,4]\n",
    "plusOne(digits)\n"
   ]
  }
